#include <stdio.h>
int main() 
{
    int n, temp, remainder, num = 0;
    printf("Enter the number: ");
    scanf("%d", &n);
    temp = n;
    while (temp != 0) 
    {
        remainder = temp % 10;// remainder contains the last digit
        num += remainder * remainder * remainder;
        temp /= 10;// remove last digit from the orignal number
    }
    if (num == n)
        printf("%d is an Armstrong number.", n);
    else
        printf("%d is not an Armstrong number.", n);
    return 0;
}
